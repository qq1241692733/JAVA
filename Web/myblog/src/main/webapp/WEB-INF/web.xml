<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1"
         metadata-complete="false">

  <!-- 用来注册servlet接口实现类 -->
  <servlet>
    <servlet-name>reg</servlet-name>
    <servlet-class>services.RegServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>reg</servlet-name>
    <url-pattern>/reg</url-pattern>
  </servlet-mapping>

  <!-- 用来登录servlet接口实现类 -->
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>services.LoginRegServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <!-- 用来登录servlet接口实现类 -->
  <servlet>
    <servlet-name>myartlist</servlet-name>
    <servlet-class>services.MyArtListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>myartlist</servlet-name>
    <url-pattern>/myartlist</url-pattern>
  </servlet-mapping>


</web-app>